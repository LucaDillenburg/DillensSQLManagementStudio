    <!-- insert into -->
    <command name="insert into">
        <stage>
            <explanation>
                This command is used to insert a new row in a specific table.
                The simple syntax is: insert into ....
            </explanation>

            <example>insert into [tableName] values()</example>
        </stage>
    </command>
</sql_comand>